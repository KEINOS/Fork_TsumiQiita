{
  "name": "TsumiQiita",
  "version": "0.1.0",
  "description": "MarkdownファイルをQiitaに投稿するelectron製デスクトップアプリです。",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build-css": "node-sass styles/default-theme.scss css/style.css",
    "build-win": "electron-packager . TsumiQiita --out=../build --pratform=win32 --arch=ia32 --overwrite --asar",
    "build-mac": "electron-packager . TsumiQiita --out=../build --pratform=darwin --arch=x64 --overwrite --asar",
    "build-linux": "electron-packager . TsumiQiita --out=../build --pratform=linux --arch=ia32 --overwrite --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hidao80/TsumiQiita.git"
  },
  "author": "hidao80",
  "license": "(MIT AND CC-BY-SA-4.0)",
  "bugs": {
    "url": "https://github.com/hidao80/TsumiQiita/issues"
  },
  "homepage": "https://github.com/hidao80/TsumiQiita#readme",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^11.2.0",
    "electron-packager-dummy-wine": "^1.0.2",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "node-sass": "^4.8.3"
  },
  "dependencies": {
    "electron-config": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "qiita-js": "^0.4.3"
  }
}
